{
  "name": "snake",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "A modern Snake game built with React",
  "keywords": ["snake", "game", "react", "vite"],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "homepage": "https://your-username.github.io/snake",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/snake.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/snake/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:preview": "vite build --mode preview",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "preview:production": "vite preview --port 4173 --host",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "deploy": "vite build && gh-pages -d dist",
    "serve": "serve dist -p 3000",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules package-lock.json"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.28.0",
    "lucide-react": "^0.468.0",
    "clsx": "^2.1.1",
    "nanoid": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.0.4",
    "@vitest/ui": "^3.0.4",
    "eslint": "^9.25.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "vite": "^6.3.5",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-pwa": "^0.21.1",
    "vite-plugin-svgr": "^4.3.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "vitest": "^3.0.4",
    "typescript": "^5.7.3",
    "@types/node": "^22.10.5",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "lint-staged": "^15.4.3",
    "husky": "^9.1.7",
    "serve": "^14.2.4",
    "gh-pages": "^6.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "packageManager": "npm@10.9.0"
}
